@page "{id}"
@model Fiever.UI.Models.DeleteStudentModel

<h2>Delete Student</h2>

<p>Are you sure you want to delete this student?</p>

<form method="post">
    <button type="submit" class="btn btn-danger">Delete</button>
    <a asp-page="/Students/Index" class="btn btn-secondary">Cancel</a>
</form>